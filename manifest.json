{
  "manifest_version": 2,
  "name": "SiteBlock - With Dark Mode",
  "version": "0.2.4",
  "description": "Block sites to discourage procrastination (auto-switch to dark mode if system is in dark mode)",
  "options_page": "html/options.html",
  "background": {
    "page": "html/background.html"
  },
  "permissions": ["tabs"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
